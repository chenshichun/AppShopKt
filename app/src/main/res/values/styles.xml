<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="merchant_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_dp_50</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="merchant_txt">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/color_333</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="merchant_et">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/color_333</item>
        <item name="android:gravity">center_vertical|right</item>
    </style>

    <style name="my_toast">
        <item name="stTextBold">true</item>
        <item name="stTextColor">#fff</item>
        <item name="stTextSize">14sp</item>
        <item name="stColorBackground">@color/color_666</item>
        <item name="stSolidBackground">true</item>
        <item name="stLength">SHORT</item>
        <item name="stGravity">center</item>
        <item name="stRadius">20dp</item>
    </style>
</resources>